{
	"name": "ndimic/laravel-dynamic-translations",
	"description": "Dynamic database translations for Laravel with automatic caching and admin panel",
	"keywords": [
		"laravel",
		"translations",
		"i18n",
		"localization",
		"database-translations",
		"dynamic-translations",
		"multi-language"
	],
	"type": "library",
	"license": "proprietary",
	"authors": [
		{
			"name": "Nenad Dimic",
			"email": "ndimicgm@gmail.com",
			"homepage": "",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.1|^8.2|^8.3",
		"illuminate/support": "^10.0|^11.0|^12.0",
		"illuminate/database": "^10.0|^11.0|^12.0",
		"illuminate/cache": "^10.0|^11.0|^12.0"
	},
	"require-dev": {
		"orchestra/testbench": "^8.0|^9.0",
		"phpunit/phpunit": "^10.0",
		"mockery/mockery": "^1.5"
	},
	"autoload": {
		"psr-4": {
			"Ndimic\\Translations\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Ndimic\\Translations\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Ndimic\\Translations\\TranslationServiceProvider"
			],
			"aliases": {
				"DynamicTrans": "Ndimic\\Translations\\Facades\\Translator"
			}
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"test-coverage": "vendor/bin/phpunit --coverage-html coverage"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}